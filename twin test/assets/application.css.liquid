.woocommerce form .form-row .required { visibility: visible; }

.variable-item:not(.radio-variable-item) { width : 30px; height : 30px; } .wvs-style-squared .button-variable-item { min-width : 30px; } .button-variable-item span { font-size : 16px; }

.site-logo {
    /*background-image: url(/img/svg/cedarcreek_logo.svg#logo)*/
    background-image: url(https://twinxl.com/wp-content/themes/twinxl/img/svg/twinxl_logo.svg#logo)
}
#header.fixed .site-logo {
    background-image: url(https://twinxl.com/wp-content/themes/twinxl/img/svg/twinxl_logo.svg#logo);
}

.section-title {
    margin-bottom: 40px
}
section.advantages {
    padding: 0;
    font-size: 1.7em;
}
.advantage-box-1,
.advantage-box-2 {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100%;
}
.advantage-box-1-text,
.advantage-box-2-text {
    padding: 60px;
}
.advantage-box-1-text :last-child,
.advantage-box-2-text :last-child {
    margin: 0;
}

/*Fifty-fifty Banner*/
#fifty-fifty-banner {
    padding: 0;
}
.fifty-image {
    
}
.fifty-text {
    padding: 10% 7%;
    background-color: #f7f7f7;
}

/*-Footer Icon Blocks-*/
.footer-icon-blocks {
    background-color: #ebf5ff;
}
.footer-icon-blocks .third {
    flex-basis: 25%;
}
.icon-block {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 30px 0;
}
.icon-block-content {
    color: #1b3564;
    text-align: center;
}
.icon-block-content i {
    display: block;
    margin-bottom: 20px;
    font-size: 4em;
}
.icon-block-title {
    font-size: 1.6em;
    margin-bottom: 10px;
}
img.icon-block-icon-img {
    max-width: 30%;
    height: auto;
    margin-bottom: 14px;
}

/*Footer Newsletter Signup Section*/
.cta-holder {
    display: flex;
    justify-content: center;
    align-items: center;
}
.footer-newsletter-signup p.h1 {
    color: #fff;
    margin: 0 60px 0 0;
}
.footer-newsletter-signup .gform_wrapper {
    margin: 0;
}
.footer-newsletter-signup .gform_wrapper form {
    display: flex;
}
.footer-newsletter-signup .gform_wrapper ul.gform_fields li.gfield {
    margin: 0;
    padding: 0;
}
body .footer-newsletter-signup  .gform_wrapper .top_label div.ginput_container {
    margin-top: 0;
}
.footer-newsletter-signup .gform_wrapper input:not([type=radio]):not([type=checkbox]):not([type=submit]):not([type=button]):not([type=image]):not([type=file]) {
    border: none;
    padding: 10px;
    width: 400px;
    height: 50px;
    margin: 0;
}
.footer-newsletter-signup .gform_wrapper .gform_footer {
    margin: 0;
    padding: 0;
}
.footer-newsletter-signup .gform_wrapper .gform_footer input.button, 
.footer-newsletter-signup .gform_wrapper .gform_footer input[type=submit] {
    padding: 10px;
    height: 50px;
    margin: 0;
    font-size: 2em;
    color: #ffffff;
    line-height: 19px;
    background: #ff5c2f;
    border-radius: 0;
}

.footer-newsletter-signup .gform_wrapper div.validation_error {
    display: none;
    color: white;
    border: none;
    text-align: left;
    margin-right: 10px;
    padding: 1px;
}
.footer-newsletter-signup .gform_wrapper li.gfield_error input:not([type=radio]):not([type=checkbox]):not([type=submit]):not([type=button]):not([type=image]):not([type=file]), 
.footer-newsletter-signup .gform_wrapper li.gfield_error textarea {
    border: none;
}
.footer-newsletter-signup .gform_wrapper.gform_validation_error .gform_body ul li.gfield.gfield_error:not(.gf_left_half):not(.gf_right_half) {
    border: none;
    background: none;
}
.footer-newsletter-signup .gform_wrapper .field_description_below .gfield_description {
    padding-top: 0px;
    color: white;
}

@media screen and (max-width: 1200px) {
    .advantage-box-1-text,
    .advantage-box-2-text {
        font-size: .8em;
    }
}
@media screen and (max-width: 980px) {
    .advantages .col.third {
        margin: 0;
    }
    .advantage-box-1-text,
    .advantage-box-2-text {
        font-size: .6em;
    }
    
    .icon-block {
        padding: 0 10%;
    }
    
    /*Newsletter Signup*/
    .cta-holder {
        display: block;
    }
    .footer-newsletter-signup .gform_wrapper input:not([type=radio]):not([type=checkbox]):not([type=submit]):not([type=button]):not([type=image]):not([type=file]) {
        width: 100%;
    }
    .footer-newsletter-signup div.gform_body {
        width: calc(100% - 50px);
    }
    .footer-newsletter-signup .gform_wrapper .gform_footer {
        width: 50px;
    }
    .footer-newsletter-signup p.h1 {
        margin: 0 0 20px 0;
    }
    .footer-newsletter-signup .gform_wrapper input:not([type=radio]):not([type=checkbox]):not([type=image]):not([type=file]) {
        line-height: 1em;
    }
    
}
@media screen and (max-width: 414px) {
     /*fifty fifth banner*/
    #fifty-fifty-banner .fifty-image-holder {
        height: 280px;
        margin: 0;
    }
}

#home-featured {
    padding: 0;
}

.box-text :last-child {
    margin-bottom: 0;
}
.box-left {
    display: flex;
    align-items: center;
    width: 100%;
    height: 500px;
    background-color: #eeeeee;
}
.box-left .box-text {
    padding: 40px 100px;
    font-size: 1.3em;
}
.box-text p.button-holder {
    font-size: 16px;
}

/*box right top*/
.box-right-top {
    display: flex;
    align-items: center;
    height: 250px;
    background-color: #eeeeee;
}
.box-right-top .box-text {
    font-size: 1.6em;
    padding: 30px 60px;
    font-weight: 800;
}

/*box right bottom*/
.box-right-bottom {
    display: flex;
    align-items: center;
    height: 250px;
    background: linear-gradient(45deg, #1f4786, #1b3564);
}
.box-right-bottom .box-text {
    font-size: 1.6em;
    padding: 30px 60px;
    font-weight: 800;
}

@media screen and (max-width: 1400px) {
    .box-left .box-text {
        padding: 20px 60px;
        font-size: 1.2em;
    }
    .box-right-top .box-text,
    .box-right-bottom .box-text {
        padding: 20px 60px;
        font-size: 1.3em;
    }	
}
@media screen and (max-width: 980px) {
    .row.top-area .col.half {
        margin-bottom: 0px;
    }
    .box-right-top,
    .box-right-bottom {
        margin-left: 0;
    }
}
@media screen and (max-width: 414px) {
    #home-featured {
        padding: 0;
    }
    
    .box-left {
        height: auto;
    }
    .box-left .box-text {
        padding: 20px;
        font-size: 1em;
    }
    .box-right-top,
    .box-right-bottom {
        height: auto;
        margin: 0;
    }
    .box-right-top .box-text, .box-right-bottom .box-text {
        padding: 20px 20px;
    }

}

